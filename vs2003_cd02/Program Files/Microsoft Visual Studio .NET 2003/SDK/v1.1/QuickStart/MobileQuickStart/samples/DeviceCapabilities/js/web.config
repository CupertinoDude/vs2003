<configuration>
  <system.web>
    <deviceFilters>

      <!-- Custom Filters for JScript examples -->

      <filter name="IsLandscape"   type="Acme.CustomFilters,Acme.CustomFiltersJS" method="IsLandscape"/>
      <filter name="IsPortrait"    type="Acme.CustomFilters,Acme.CustomFiltersJS" method="IsPortrait"/>
      <filter name="IsSmallScreen" type="Acme.CustomFilters,Acme.CustomFiltersJS" method="IsSmallScreen"/>
      <filter name="IsBigScreen"   type="Acme.CustomFilters,Acme.CustomFiltersJS" method="IsBigScreen"/>

      <filter name="UsingChtmlRendering" type="Acme.CustomFilters,Acme.CustomFiltersJS" method="UsingChtmlRendering"/>

      <filter name="GoWebOnPalm"       type="Acme.CustomFilters,Acme.CustomFiltersJS" method="GoWebOnPalm"/>
      <filter name="GoWebOnRIM"        type="Acme.CustomFilters,Acme.CustomFiltersJS" method="GoWebOnRIM"/>
      <filter name="MmeOnSony"         type="Acme.CustomFilters,Acme.CustomFiltersJS" method="MmeOnSony"/>
      <filter name="MmeOnBenefon"      type="Acme.CustomFilters,Acme.CustomFiltersJS" method="MmeOnBenefon"/>
      <filter name="ImodeOnMitsubishi" type="Acme.CustomFilters,Acme.CustomFiltersJS" method="ImodeOnMitsubishi"/>
      <filter name="ImodeOnFujitsu"    type="Acme.CustomFilters,Acme.CustomFiltersJS" method="ImodeOnFujitsu"/>

      <filter name="UseSmallGif"   type="Acme.CustomFilters,Acme.CustomFiltersJS" method="UseSmallGif"/>
      <filter name="UseMediumGif"  type="Acme.CustomFilters,Acme.CustomFiltersJS" method="UseMediumGif"/>
      <filter name="UseLargeGif"   type="Acme.CustomFilters,Acme.CustomFiltersJS" method="UseLargeGif"/>
      <filter name="UseSmallWbmp"  type="Acme.CustomFilters,Acme.CustomFiltersJS" method="UseSmallWbmp"/>
      <filter name="UseMediumWbmp" type="Acme.CustomFilters,Acme.CustomFiltersJS" method="UseMediumWbmp"/>
      <filter name="UseLargeWbmp"  type="Acme.CustomFilters,Acme.CustomFiltersJS" method="UseLargeWbmp"/>
      <filter name="UseSmallBmp"   type="Acme.CustomFilters,Acme.CustomFiltersJS" method="UseSmallBmp"/>
      <filter name="UseMediumBmp"  type="Acme.CustomFilters,Acme.CustomFiltersJS" method="UseMediumBmp"/>
      <filter name="UseLargeBmp"   type="Acme.CustomFilters,Acme.CustomFiltersJS" method="UseLargeBmp"/>

    </deviceFilters>
  </system.web>
</configuration>
